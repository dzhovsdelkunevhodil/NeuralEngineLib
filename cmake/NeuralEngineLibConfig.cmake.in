@PACKAGE_INIT@

include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@Targets.cmake")

check_required_components(NeuralEngineLib)

if(NOT DEFINED NEURAL_ENGINE_USE_GPU)
    set(NEURAL_ENGINE_USE_GPU @NEURAL_ENGINE_USE_GPU@)
endif()

if(NOT DEFINED NEURAL_ENGINE_ENABLE_LOGGING)
    set(NEURAL_ENGINE_ENABLE_LOGGING @NEURAL_ENGINE_ENABLE_LOGGING@)
endif()

if(NEURAL_ENGINE_USE_GPU)
    set_property(TARGET NeuralEngineLib::Backend APPEND PROPERTY INTERFACE_COMPILE_DEFINITIONS NEURAL_ENGINE_USE_GPU=1)
else()
    set_property(TARGET NeuralEngineLib::Backend APPEND PROPERTY INTERFACE_COMPILE_DEFINITIONS NEURAL_ENGINE_USE_GPU=0)
endif()

if(NEURAL_ENGINE_ENABLE_LOGGING)
    set_property(TARGET NeuralEngineLib::Utils APPEND PROPERTY INTERFACE_COMPILE_DEFINITIONS NEURAL_ENGINE_LOGGING=1)
else()
    set_property(TARGET NeuralEngineLib::Utils APPEND PROPERTY INTERFACE_COMPILE_DEFINITIONS NEURAL_ENGINE_LOGGING=0)
endif()